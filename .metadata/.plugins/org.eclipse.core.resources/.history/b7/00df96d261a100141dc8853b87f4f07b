#include <stdio.h>
#include <sys/wait.h>

const unsigned int MAXLINE = 1024;

int main(int argc, char *argv[])
{
	char buf[MAXLINE];
	pid_t pid;
	int status;

	printf("%% ");

	while( fgets(buf, MAXLINE, stdin) != NULL )
	{
		if( buf[strlen(buf) - 1] == '\n')
			buf[strlen(buf) - 1] = 0;

		if( (pid = fork()) < 0 )
		{
			perror("fork");
		}
	}

	return 0;
}
